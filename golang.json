{
  "packageRules": [
    {
      "matchDatasources": [
        "golang-version"
      ],
      "rangeStrategy": "bump"
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "postUpdateOptions": [
        "gomodTidy"
      ],
      "groupName": "Go modules"
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchPackageNames": [
        "github.com/aws/aws-sdk-go"
      ],
      "groupName": null
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchPackageNames": [
        "go"
      ],
      "groupName": null
    }
  ]
}
